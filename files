│   └── readme/               # README CHW Copilot (the internal doc)
│
├── team/                     # All content relating to team assignments/org
│   ├── rosters/              # Spring 2025 Team roster
│   ├── interface-team/       # Interface Team folder
│   ├── teams/                # Teams directory
│   └── ideas/                # Idea notes
│
├── research/                 # Articles, references, regulatory info
│   ├── articles/             # “Article on ChatCHW”, ASHA & AI, etc.
│   ├── references/           # References folder
│   └── ethics/               # Ethics and regulation folder
│
├── presentations/            # Slides, demos, and presentation files
│   ├── demos/                # Demos folder
│   └── presentations/        # Presentations folder
│
├── procedures/               # Procedures + documentation for workflows
│   ├── documentation/        # Documentation folder
│   └── procedures/           # Procedures folder
│
├── spreadsheets/             # All Google Sheets exports
│   ├── grading-rubrics/      # Copy of grading rubric ChatCHW
│   └── misc/                 # Untitled spreadsheet, team spreadsheets
│
├── archived/                 # Old/retired content
│   └── Archive/              # The #Archive folder from Drive
│
├── README.md                 # Main GitHub README (I can write it for you)
